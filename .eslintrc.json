{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "ignorePatterns": [
    "node_modules/",
    "admin/",
    "lib/node-mideahvac/"
  ],
  "rules": {
    "no-unused-vars": [
      "warn",
      { "args": "none" }
    ],
    "no-console": "off"
  }
}
